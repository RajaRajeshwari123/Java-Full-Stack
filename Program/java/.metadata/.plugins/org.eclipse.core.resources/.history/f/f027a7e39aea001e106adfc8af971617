package New_Project.stringexamples;

public class StringAssignment {
	public static void main(String args[]) {
		 for (int i = 0; i < str.length(); i++) {
	            char ch = str.charAt(i);
	            if (ch < '0' || ch > '9') {
	                return false;
	            }
	        }
	        return true;
	    }

	    public static void main(String[] args) {
	        String input1 = "ABC1234";
	        String input2 = "Hello";
	        String input3 = "3456";

	        System.out.println("Input 1: " + onlyDigits(input1)); // Output: false
	        System.out.println("Input 2: " + onlyDigits(input2)); // Output: false
	        System.out.println("Input 3: " + onlyDigits(input3)); // Output: true
	    }
	    class DigitChecker {
	        public static boolean onlyDigits(String str) {
	            for (int i = 0; i < str.length(); i++) {
	                if (!Character.isDigit(str.charAt(i))) {
	                    return false;
	                }
	            }
	            return true;
	        }

	        public static void main(String[] args) {
	            // Same input strings as before
	            // ...
	        }
	    }
			
		
	}


