package New_Project.stringexamples;

public class StringAnotherDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//03451627818363531789
		String helloworld="Kaise Hoo? Elikutty";
		System.out.println(helloworld.length());
		System.out.println(helloworld.charAt(15));
		System.out.println(helloworld.indexOf('1'));
		System.out.println("************************");
		for(int index=0;index<helloworld.length();index++) {
			if((helloworld.charAt(index))=='1'){
				System.out.println(index);
				
			}
		}
		System.out.println("**************************************");
		helloworld.concat("\\n world is very beautiful!!!");
		System.out.println(helloworld);

	}

}
